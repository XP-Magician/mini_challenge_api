{
  "version": 2,
  "builds": [{ "src": "./index.mjs", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Authorization"
      },
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "/"
    }
  ]
}
